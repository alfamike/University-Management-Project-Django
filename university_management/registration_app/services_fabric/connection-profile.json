{
  "name": "fabric-network",
  "description": "Basic network with 1 Org, 1 Orderer and 2 Peers",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "credentialStore": {
      "path": "crypto_store/hfc-kvs",
      "cryptoStore": {
        "path": "crypto_store/hfc-cvs"
      },
      "wallet": "crypto_store/wallet"
    }
  },
  "channels": {
    "registration-channel": {
      "orderers": [
        "orderer.university.eu:7050"
      ],
      "peers": [
        "peer0.org1.university.eu:7051",
        "peer1.org1.university.eu:8051"
      ]
    }
  },
  "organizations": {
    "OrdererOrg": {
      "mspid": "OrdererMSP",
      "orderers": [
        "orderer.university.eu"
      ],
      "Org1": {
        "mspid": "Org1MSP",
        "peers": [
          "peer0.org1.university.eu",
          "peer1.org1.university.eu"
        ],
        "users": {
          "Admin": {
            "cert": "certs/Admin@org1.university.eu-cert.pem",
            "private_key": "certs/admin_org1_key"
          }
        },
        "certificateAuthorities": [
          "ca-org1"
        ]
      }
    }
  },
  "peers": {
    "peer0.org1.university.eu": {
      "url": "grpc://peer0.org1.university.eu:7051",
      "tlsCACerts": {
        "path": "certs/ca-peers.crt"
      }
    },
    "peer1.org1.university.eu": {
      "url": "grpc://peer1.org1.university.eu:8051",
      "tlsCACerts": {
        "path": "certs/ca-peers.crt"
      }
    }
  },
  "orderers": {
    "orderer.university.eu": {
      "url": "grpc://orderer.university.eu:7050",
      "tlsCACerts": {
        "path": "certs/orderer-tls-ca.pem"
      }
    }
  },
  "certificateAuthorities": {
    "ca-org1": {
      "url": "http://localhost:7054",
      "grpcOptions": {
        "verify": true
      },
      "registrar": [
        {
          "enrollId": "admin",
          "enrollSecret": "adminpw"
        }
      ]
    }
  }
}
